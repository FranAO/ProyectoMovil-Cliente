<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="80dp">

    <View
        android:id="@+id/sliding_indicator"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/sliding_indicator_bg"
        android:elevation="2dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/nav_button_1"
        app:layout_constraintStart_toStartOf="@+id/nav_button_1"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/nav_button_1"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:elevation="4dp"
        app:layout_constraintEnd_toStartOf="@+id/nav_button_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/icon_1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@drawable/casa"
            android:scaleType="centerInside"
            app:tint="#9CADBA"
            app:layout_constraintBottom_toTopOf="@+id/label_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <TextView
            android:id="@+id/label_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Home"
            android:textColor="#9CADBA"
            android:textSize="10sp"
            android:fontFamily="sans-serif-medium"
            android:alpha="0.7"
            android:maxWidth="50dp"
            android:ellipsize="end"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/icon_1" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/nav_button_2"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:elevation="4dp"
        app:layout_constraintEnd_toStartOf="@+id/nav_button_3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/nav_button_1"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/icon_2"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@drawable/ruta"
            android:scaleType="centerInside"
            app:tint="#9CADBA"
            app:layout_constraintBottom_toTopOf="@+id/label_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <TextView
            android:id="@+id/label_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Route"
            android:textColor="#9CADBA"
            android:textSize="10sp"
            android:fontFamily="sans-serif-medium"
            android:alpha="0.7"
            android:maxWidth="50dp"
            android:ellipsize="end"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/icon_2" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/nav_button_3"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:elevation="4dp"
        app:layout_constraintEnd_toStartOf="@+id/nav_button_4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/nav_button_2"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/icon_3"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@drawable/bus"
            android:scaleType="centerInside"
            app:tint="#9CADBA"
            app:layout_constraintBottom_toTopOf="@+id/label_3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <TextView
            android:id="@+id/label_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Buses"
            android:textColor="#9CADBA"
            android:textSize="10sp"
            android:fontFamily="sans-serif-medium"
            android:alpha="0.7"
            android:maxWidth="50dp"
            android:ellipsize="end"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/icon_3" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/nav_button_4"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:elevation="4dp"
        app:layout_constraintEnd_toStartOf="@+id/nav_button_5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/nav_button_3"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/icon_4"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@drawable/historial"
            android:scaleType="centerInside"
            app:tint="#9CADBA"
            app:layout_constraintBottom_toTopOf="@+id/label_4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <TextView
            android:id="@+id/label_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="History"
            android:textColor="#9CADBA"
            android:textSize="10sp"
            android:fontFamily="sans-serif-medium"
            android:alpha="0.7"
            android:maxWidth="50dp"
            android:ellipsize="end"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/icon_4" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/nav_button_5"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:elevation="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/nav_button_4"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/icon_5"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@drawable/advertencia"
            android:scaleType="centerInside"
            app:tint="#9CADBA"
            app:layout_constraintBottom_toTopOf="@+id/label_5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <TextView
            android:id="@+id/label_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Report"
            android:textColor="#9CADBA"
            android:textSize="10sp"
            android:fontFamily="sans-serif-medium"
            android:alpha="0.7"
            android:maxWidth="50dp"
            android:ellipsize="end"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/icon_5" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</merge>